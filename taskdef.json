{
  "executionRoleArn": "arn:aws:iam::362708816803:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "webapp",
      "image": "<IMAGE>",
      "essential": "true",
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/healthcheck || exit 1"
        ],

        "interval": 5,
        "timeout": 2,
        "retries": 1,
        "startPeriod": 0
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "family": "wsi-taskdef-stress",
  "networkMode": "awsvpc"
}

